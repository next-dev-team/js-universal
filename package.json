{
  "name": "electron-vite-react",
  "version": "1.0.0",
  "description": "A project template based on Electron, React 19, TypeScript, and Vite 6, designed to help quickly set up React applications.",
  "keywords": [
    "electron",
    "react19 ts template",
    "react template",
    "react",
    "template",
    "vite"
  ],
  "homepage": "https://github.com/next-dev-team/electron-vite",
  "bugs": "https://github.com/next-dev-team/electron-vite/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/next-dev-team/electron-vite"
  },
  "license": "MIT",
  "author": "sila",
  "type": "module",
  "main": "./dist-electron/main/index.cjs",
  "scripts": {
    "build:dev": "electron-vite build -m dev",
    "build:pro": "electron-vite build -m pro",
    "build:pro:mac": "npm run build:pro && electron-builder --mac",
    "build:pro:win": "npm run build:pro && electron-builder --win",
    "build:test": "npm run electron-vite build -m test",
    "coverage": "vitest run --coverage",
    "dev": "concurrently \"electron-vite -m dev\" \"cd pinokiod && yarn start\"",
    "dev:mock": "electron-vite -m dev-mock",
    "dev:mock-server": "npm run mock-server -c mock.config.ts",
    "dev:mock:web": "vite -m dev-mock",
    "dev:pinokiod": "cd pinokiod && npm run start && cd ..",
    "dev:pro": "electron-vite -m pro",
    "dev:test": "electron-vite -m test",
    "e2e": "playwright test",
    "postinstall": "npx patch-package",
    "kill-port": "kill-port 5174",
    "lint:all": "npm run lint:eslint && npm run lint:style && npm run lint:format",
    "lint:eslint": "eslint --max-warnings 0  \"**/*.{ts,tsx,js,jsx,cjs,mjs}\" --fix",
    "lint:format": "prettier --write \"**/*.{ts,tsx,js,jsx,cjs,mjs,html,less,scss,css,json}\"",
    "lint:style": "stylelint \"**/*.{css,less,scss}\" --fix",
    "prepare": "husky",
    "preview:dev": "npm run electron-vite preview -m dev",
    "preview:pro": "npm run electron-vite preview -m pro",
    "preview:test": "npm run electron-vite preview -m test",
    "quality:check": "npm run typecheck && npm run lint:all && npm run test",
    "quality:fix": "npm run lint:eslint && npm run lint:style && npm run lint:format",
    "rebuild:native": "electron-rebuild",
    "test": "vitest run",
    "test-u": "vitest -u run",
    "test:all": "npm run test && npm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "typecheck": "tsc --noEmit",
    "update:deps": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@types/uuid": "^10.0.0",
    "ahooks": "^3.9.0",
    "better-sqlite3": "^12.2.0",
    "chokidar": "^4.0.3",
    "electron-log": "^5.3.3",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.6.2",
    "fast-glob": "^3.3.2",
    "koffi": "^2.11.0",
    "monaco-editor": "^0.52.2",
    "pterm": "^0.0.14",
    "react-router-dom": "^7.6.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/prompt-cli": "^19.8.0",
    "@electron-toolkit/utils": "^4.0.0",
    "@eslint/js": "^9.24.0",
    "@liangskyli/axios-request": "^0.2.0",
    "@liangskyli/mock": "^6.0.0",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4.1.4",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/chokidar": "^2.1.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/coverage-v8": "^3.2.4",
    "antd": "^5.26.1",
    "axios": "^1.8.4",
    "classnames": "^2.5.1",
    "concurrently": "^9.0.0",
    "electron": "^36.5.0",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "immer": "^10.1.1",
    "jsdom": "^26.1.0",
    "kill-port": "^2.0.1",
    "less": "^4.3.0",
    "lint-staged": "^16.1.2",
    "optics-ts": "^2.4.1",
    "patch-package": "^8.0.0",
    "playwright": "^1.54.1",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.10",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react": "^19.1.0",
    "react-dev-inspector": "^2.0.1",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.0",
    "sass": "^1.86.3",
    "stylelint": "^16.18.0",
    "stylelint-config-recess-order": "^7.1.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-less": "^3.0.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-order": "^7.0.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.2.6",
    "vite-plugin-checker": "^0.9.1",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.2.4",
    "zustand": "^5.0.3"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": "^20.0.0 || >=22.0.0"
  }
}
