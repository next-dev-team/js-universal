{
  "name": "electron-plugin-app",
  "private": true,
  "packageManager": "pnpm@10.19.0",
  "version": "0.0.0",
  "description": "A modern Electron app with plugin system built with React and TypeScript",
  "author": "Electron Plugin App Team",
  "type": "module",
  "main": "out/main/index.js",
  "homepage": "./",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "release": "npm run build && electron-builder --publish=never",
    "release:dir": "npm run build && electron-builder --dir --publish=never",
    "release:win": "npm run build && electron-builder --win --publish=never",
    "release:mac": "npm run build && electron-builder --mac --publish=never",
    "release:linux": "npm run build && electron-builder --linux --publish=never",
    "release:all": "npm run build && electron-builder --win --mac --linux --publish=never",
    "server:dev": "tsx api/server.ts",
    "dev:all": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "lint": "eslint .",
    "check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:api": "vitest run tests/api",
    "test:integration": "vitest run tests/integration",
    "test:unit": "vitest run tests/main",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "pinokiod:setup": "cd pinokiod && npm install && cd ..",
    "postinstall": "npm run db:generate && patch-package",
    "build:workspace": "turbo run build",
    "lint:workspace": "turbo run lint",
    "test:workspace": "turbo run test"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@prisma/client": "^6.1.0",
    "antd": "^5.22.6",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "electron-is-dev": "^3.0.1",
    "express": "^4.21.2",
    "lucide-react": "^0.511.0",
    "multer": "^1.4.5-lts.1",
    "pinokiod": "^3.170.0",
    "pterm": "^0.0.14",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0",
    "tailwind-merge": "^3.0.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.30",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vercel/node": "^2.3.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "concurrently": "^9.2.0",
    "electron": "^38.3.0",
    "electron-builder": "^25.1.8",
    "electron-vite": "^5.0.0-beta.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "happy-dom": "^20.0.7",
    "patch-package": "^8.0.1",
    "postcss": "^8.5.3",
    "prisma": "^6.1.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^7.1.11",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "turbo": "2.5.8"
  },
  "turbo": {
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**", "out/**", "build/**"]
      },
      "lint": {
        "dependsOn": ["^lint"]
      },
      "test": {
        "dependsOn": ["^test"],
        "outputs": ["coverage/**"]
      },
      "dev": {
        "cache": false
      }
    }
  },
  "build": {
    "appId": "com.superapp.electron-plugin-app",
    "productName": "Super App",
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "files": [
      "out/**/*",
      "node_modules/**/*",
      "node_modules/.prisma/**/*",
      "node_modules/@prisma/client/**/*",
      "package.json",
      "!node_modules/**/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!node_modules/**/{test,__tests__,tests,powered-test,example,examples}",
      "!node_modules/**/*.d.ts",
      "!node_modules/**/.bin",
      "!node_modules/@homebridge/node-pty-prebuilt-multiarch/**/*",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "build/icon.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "build/icon.ico",
      "sign": false,
      "signAndEditExecutable": false,
      "signDlls": false
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "build/icon.png",
      "category": "Development"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "compression": "maximum",
    "removePackageScripts": true,
    "npmRebuild": false,
    "nodeGypRebuild": false,
    "buildDependenciesFromSource": false,
    "forceCodeSigning": false,
    "asarUnpack": [
      "node_modules/@homebridge/node-pty-prebuilt-multiarch/**/*",
      "node_modules/.prisma/**/*",
      "node_modules/@prisma/client/**/*"
    ],
    "extraResources": [
      {
        "from": "node_modules/@homebridge/node-pty-prebuilt-multiarch",
        "to": "node_modules/@homebridge/node-pty-prebuilt-multiarch",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "prisma/dev.db",
        "to": "prisma/dev.db",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "node_modules/.prisma",
        "to": "node_modules/.prisma",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "node_modules/@prisma/client",
        "to": "node_modules/@prisma/client",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
