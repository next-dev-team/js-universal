{
  "name": "electron-plugin-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-main/main.js",
  "homepage": "./",
  "scripts": {
    "client:dev": "vite",
    "build": "tsc -b && vite build && vite build --config vite.main.config.ts && vite build --config vite.preload.config.ts && electron-builder",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "server:dev": "nodemon",
    "dev": "npm run electron:dev",
    "electron:dev": "concurrently \"npm run client:dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:api": "vitest run tests/api",
    "test:integration": "vitest run tests/integration",
    "test:unit": "vitest run tests/main",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "pinokiod:setup": "cd pinokiod && npm install && cd ..",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@prisma/client": "^6.1.0",
    "antd": "^5.22.6",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "electron-is-dev": "^3.0.1",
    "express": "^4.21.2",
    "lucide-react": "^0.511.0",
    "multer": "^1.4.5-lts.1",
    "pinokiod": "^3.170.0",
    "pterm": "^0.0.14",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0",
    "tailwind-merge": "^3.0.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.30",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vercel/node": "^2.3.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "concurrently": "^9.2.0",
    "electron": "^38.3.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "happy-dom": "^20.0.7",
    "nodemon": "^3.1.10",
    "patch-package": "^8.0.1",
    "postcss": "^8.5.3",
    "prisma": "^6.1.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^7.1.11",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.1"
  },
  "build": {
    "appId": "com.superapp.electron-plugin-app",
    "productName": "Super App",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist-renderer/**/*",
      "dist-main/**/*",
      "dist-preload/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
