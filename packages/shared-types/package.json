{
  "name": "@js-universal/shared-types",
  "version": "1.0.0",
  "type": "module",
  "description": "Shared TypeScript type definitions for JS Universal packages",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "24.9.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.6",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
