{
  "name": "@js-universal/shared-utils",
  "version": "1.0.0",
  "description": "Shared utility functions for the JS Universal monorepo",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/types/index.d.ts"
    },
    "./array": {
      "import": "./dist/esm/array.js",
      "require": "./dist/cjs/array.js",
      "types": "./dist/esm/types/array.d.ts"
    },
    "./object": {
      "import": "./dist/esm/object.js",
      "require": "./dist/cjs/object.js",
      "types": "./dist/esm/types/object.d.ts"
    },
    "./string": {
      "import": "./dist/esm/string.js",
      "require": "./dist/cjs/string.js",
      "types": "./dist/esm/types/string.d.ts"
    },
    "./date": {
      "import": "./dist/esm/date.js",
      "require": "./dist/cjs/date.js",
      "types": "./dist/esm/types/date.d.ts"
    },
    "./validation": {
      "import": "./dist/esm/validation.js",
      "require": "./dist/cjs/validation.js",
      "types": "./dist/esm/types/validation.d.ts"
    },
    "./async": {
      "import": "./dist/esm/async.js",
      "require": "./dist/cjs/async.js",
      "types": "./dist/esm/types/async.d.ts"
    },
    "./dom": {
      "import": "./dist/esm/dom.js",
      "require": "./dist/cjs/dom.js",
      "types": "./dist/esm/types/dom.d.ts"
    },
    "./storage": {
      "import": "./dist/esm/storage.js",
      "require": "./dist/cjs/storage.js",
      "types": "./dist/esm/types/storage.d.ts"
    },
    "./url": {
      "import": "./dist/esm/url.js",
      "require": "./dist/cjs/url.js",
      "types": "./dist/esm/types/url.d.ts"
    },
    "./math": {
      "import": "./dist/esm/math.js",
      "require": "./dist/cjs/math.js",
      "types": "./dist/esm/types/math.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "dev": "tsc -p tsconfig.esm.json --watch",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "utilities",
    "helpers",
    "typescript",
    "javascript",
    "monorepo"
  ],
  "author": "JS Universal Team",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@js-universal/shared-config": "workspace:*",
    "@js-universal/shared-types": "workspace:*",
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {},
  "publishConfig": {
    "access": "public"
  }
}